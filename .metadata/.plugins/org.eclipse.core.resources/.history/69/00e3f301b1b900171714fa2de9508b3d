package rough;

import java.util.Scanner;

public class AfterHoliday {
	
	
	public static void main(String[] args) {
	
		int a[]={10,30,24,21,43,22};
		int temp=0;
		for (int i = 0; i < a.length; i++) {
			for (int j = 0; j < a.length; j++) {
				
			
			if(a[j]>a[j+1])
			{
				temp=a[j];
				a[j]=a[j+1];
				a[j+1]=temp;
			}
			
		}
			for (int j = 0; j <= a.length; j++) {
				System.out.println(a[j]);
				
			}
		}
	
	
	
		
	}
	
		
	}


