package BatlaList;

public class Linklist {

	Node head;
	
	public Linklist() {
		Node node = new Node();
		node.setNext(null);
		head=node;
	}
	void add(int data)
	{
		Node node = new Node();
		node.setData(data);
		node.setNext(null);
		if(head==null)
		{
			head=node;  
		}
		else
		{
			head.next=node;
		}
	
	}
	
} 
