package sort;

public class SelectionSort {
	public static void main(String[] args) {
		
		int a[] ={11,23,4,22,12,5,2,18};
		
		int len = a.length;
		for (int i = 0; i < len-1; i++) {
			int minValue = i;
			
			for (int j = i+1; j < len; j++) {

				{
					if(a[j]<a[minValue])
						minValue=j;
				}
				
				int temp = a[minValue];
				a[minValue] = a[i];
				a[i]=temp;
				
			}
						
		}
		
	}


}
