import java.util.HashMap;
import java.util.Map;

public class Rough 
{
	
	public static void main(String[] args) 
	{
		
	String a = "abcaba";
	char ch[] = a.toCharArray();
	Map m = new HashMap();
	
	for (int i = 0; i < ch.length; i++) 
	{
		char temp = a.charAt(i);
		int count = 0;	
		for (int j = 0; j < ch.length; j++) 
		{
			if(temp==a.charAt(j))
			{
				count++;
			}
		}
		
		m.put(temp, count);
	}
	System.out.println(m);
	
		
	}
		
	}

	
		
		
		
		
		
		
