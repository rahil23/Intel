package Rough;

public class Rough
{
	public static void main(String[] args) {
		
		String s = "Hello World";
		 char ch[] = s.toCharArray();
		 String rev = "";
		 
		 int length  = ch.length;
		 
		 for (int i = length-1 ;i>=0;i-- )
		 {
			 rev = rev+s.charAt(i);
		 }
		 System.out.println(rev);
		 
		
		
	}
		
	
}