package sort;

public class SelectionSort {
	public static void main(String[] args) {
		
		int a[] ={11,23,4,22,12,5,2,18};
		
		int len = a.length;
		for (int i = 0; i < len; i++) {
			int minValue = i;
			
				for (int j = i; j < len; j++) {
				{
					if(a[j] > a[minValue])
					{	minValue=j;
					
					int temp = a[minValue];
					a[minValue] = a[i];
					a[i]=temp;
					}
				}
				
				
				
			}
			//System.out.println(a[i]);
						
		}
		
		for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		}
	}


}
