package rough;

import java.util.Scanner;

public class AfterHoliday {
	
	
	public static void main(String[] args) {
	int a=0,b=1,c;
	int fact=1;
	Scanner sc = new Scanner(System.in);
	int n =sc.nextInt();
	
	for (int i = 1; i < n; i++) {
		fact = fact*i;
System.out.println("Factorial of"+i+"is : "+fact);
		}
	
	
	
		
	}
	
		
	}


