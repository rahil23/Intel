

package sort;

public class Simplesorting {

	public static void main(String[] args) {
		int a[] = {0,1,0,1,1,0,0,1,0,0};
		
		int m[]= new int[a.length];

		int start=0;
		int end=a.length;
		for (int i = 0; i < a.length; i++) {
			
			if(a[i]==0)
			{
				
				start++;
			}
			else if(a[i]==1)
			{
				end--;
			}
			
		}
	}
}
