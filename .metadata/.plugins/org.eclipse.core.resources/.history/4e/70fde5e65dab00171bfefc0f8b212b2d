package Array;

public class MissingNumber {
	
	public static void main(String[] args) {
		int a[] = {1,2,4,5,6,7};
		int len = a.length;
		int count = 0;
		
		for (int i = 0; i < a.length; i++) {
			
			if(a[i] != (count+1))
			{
				System.out.println("missing no"+i);
			}
			count++;
			
		}
	}

}
