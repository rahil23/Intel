
public class fibbo {

	public static void main(String[] args) {
		String s = "aaabcbbccdacd";
		char ch[] = s.toCharArray();
		
		for (int i = 0; i < ch.length-1; i++) {
			for (int j = i; j < ch.length-1; j++) {
				if(ch[j]==ch[j+1])
				{
					System.out.println(ch[j]);
				}
			}
		}
		
		
		
	}
}
