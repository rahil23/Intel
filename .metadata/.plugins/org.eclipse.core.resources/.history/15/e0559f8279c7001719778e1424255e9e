import java.util.HashMap;
import java.util.Map;

public class Rough 
{
	
	public static void main(String[] args) 
	{
		
int ar[]={1,1,1,1};
Map  m = new HashMap();
int count=0;
int len = ar.length-1;
for (int i = 0; i <=len; i++) {
	
	for (int j = i; j <=len; j++) {

		if(ar[i]+ar[j]==2)
		{
			count++;
			System.out.println(i+ " "+j);
		}
	}

	
}
System.out.println(count);	

		}
	}

	
		
		
		
		
		
		
