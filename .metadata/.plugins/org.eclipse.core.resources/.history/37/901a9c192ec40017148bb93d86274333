import java.util.HashMap;
import java.util.Map;

public class Rough 
{
	
	public static void main(String[] args) 
	{
		
String a  = "karak";
int len = a.length()-1;
for (int i = 0; i < len; i++) {
	if(a.charAt(i)!=a.charAt(len))
	{
		System.out.println("not a anagram");
		break;
	}
	
}
		
	}
		
	}

	
		
		
		
		
		
		
