import java.util.HashMap;
import java.util.Map;

public class Rough 
{
	
	public static void main(String[] args) 
	{
		
int ar[]={1,2,3,4,1,2,3,5};
Map  m = new HashMap();
int count=0;
int len = ar.length-1;
for (int i = 0; i <=len; i++) {
	
	for (int j = i; j <=len; j++) {

		if(ar[i]+ar[j]==6)
		{
			count++;
			System.out.println(ar[i]+ " "+ar[j]);
		}
	}

	
}
System.out.println(count);	

		}
	}

	
		
		
		
		
		
		
