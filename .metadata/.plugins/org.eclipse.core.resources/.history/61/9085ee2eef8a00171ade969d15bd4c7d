	public static void main(String[] args) {
		String g="google";
		char c[]= g.toCharArray();
		for (int i = 1; i <= c.length; i++) {
			{
				for (int j = 0; j < i; j++) {
					System.out.println(c[j]);
				}
				System.out.println();
				}
			}
			
		}
	}
}
